        </tbody>
    </table>

    <a href="{{ path('app_categorie_index') }}"class="btn btn-primary btn-custom " style="width:200px;  white-space: nowrap; "><i class="fas fa-arrow-left"></i> Liste des Categories</a>
        <a href="{{ path('app_home_admin') }}"class="btn btn-primary btn-custom" style="width:200px;  white-space: nowrap; float: right; "><i class="fas fa-arrow-right"></i> Dashboard</a>

          


    
