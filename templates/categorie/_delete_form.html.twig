<form method="post" action="{{ path('app_categorie_delete', {'idCategorie': categorie.idCategorie}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ categorie.idCategorie) }}">
    <button class="btn btn-primary btn-custom" style="background-color: #BB0B0B;border-color: #BB0B0B"><i class="fas fa-trash-alt"></i>  Supprimer </button>
</form>
